generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model User {
    id        Int      @id @default(autoincrement())
    clerkId   String   @unique
    email     String   @unique
    name      String?  @unique
    avatar    String?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
